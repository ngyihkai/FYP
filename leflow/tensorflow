# reinstall python
sudo apt-get remove python3-pip
wget https://bootstrap.pypa.io/pip/3.5/get-pip.py
python3 get-pip.py
python3 -m pip list

curl https://bootstrap.pypa.io/pip/2.7/get-pip.py --output get-pip.py   
python2 get-pip.py
python2 -m pip list